<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>3</defaultcontrol>
	<!-- 	
ID 	Type 			Description
2 	label 			Heading label
3 	grouplist 		Grouplist template
5 	grouplist 		Grouplist template
7 	button 			Button template
8 	radiobutton 	Radiobutton template
9 	spincontrolex 	Spincontrolex template
10 	button 			Button template
11 	image 			Default separator image
12 	edit 			Edit template
13 	sliderex 		Slider template
14 	label 			Label template
28 	button 			OK button
29 	button 			Cancel button
30 	button 			<unused button>  
-->
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<!-- default group -->
		<control type="group">
			<!--#######################################################################   MENU DA DIREITA   #######################################################################-->
			<control type="group">
				<left>0</left>
				<animation effect="slide" start="1900,0" end="0,0" time="1000" tween="cubic" easing="out">WindowOpen</animation>
				<animation effect="slide" start="0,0" end="1900,0" time="1000" tween="cubic" easing="out">WindowClose</animation>
				<animation effect="slide" start="900,0" end="0,0" time="1000" tween="cubic" easing="out">Visible</animation>
				<animation effect="fade" start="100" end="0" time="400">Hidden</animation>
				<control type="image">
					<texture>dialog-bg.png</texture>
				</control>
				<control type="grouplist" id="3">
					<description>Categories Area</description>
					<left>100</left>
					<top>100</top>
					<width>400</width>
					<height>690</height>
					<itemgap></itemgap>
					<orientation>vertical</orientation>
					<onleft>9000</onleft>
					<onright>5</onright>
					<onup>3</onup>
					<ondown>3</ondown>
				</control>
				<control type="button" id="10">
					<description>Default Category Button</description>
					<height>90</height>
					<width>390</width>
					<textoffsetx>20</textoffsetx>
					<align>left</align>
					<aligny>center</aligny>
					<font>font30</font>
					<texturefocus>focus.png</texturefocus>
					<focusedcolor>black</focusedcolor>
				</control>
				<control type="grouplist" id="5">
					<description>control area</description>
					<left>500</left>
					<top>100</top>
					<width>1320</width>
					<height>810</height>
					<onleft>3</onleft>
					<onright>28</onright>
					<onup>5</onup>
					<ondown>5</ondown>
				</control>
				<control type="textbox" id="6">
					<description>description area</description>
					<left>50</left>
					<top>0</top>
					<right>50</right>
					<height>70</height>
					<font>font12</font>
					<align>justify</align>
					<textcolor>button_focus</textcolor>
					<autoscroll time="3000" delay="5000" repeat="5000">true</autoscroll>
				</control>
				<control type="button" id="7">
					<description>Default Button</description>
					<include>button_90</include>
				</control>
				<control type="radiobutton" id="8">
					<description>Default RadioButton</description>
					<include>onoffbutton</include>
					<include>button_90</include>
				</control>
				<control type="spincontrolex" id="9">
					<description>Default SpinControlex</description>
					<include>updownbutton</include>
					<include>button_90</include>
				</control>
				<control type="image" id="11">
					<description>separator image</description>
					<height>3</height>
					<texture colordiffuse="AAAAAAAA" border="3">dialogs/separator-grey.png</texture>
				</control>
				<control type="edit" id="12">
					<left>0</left>
					<top>0</top>
					<height>75</height>
					<label></label>
					<align>center</align>
					<aligny>center</aligny>
					<font>font25_title</font>
					<textcolor>grey</textcolor>
				</control>
				<control type="sliderex" id="13">
					<description>Default Slider</description>
					<posx>80</posx>
					<textoffsetx>40</textoffsetx>
					<include>button_90</include>
					<textwidth>700</textwidth>
					<sliderwidth>200</sliderwidth>
					<sliderheight>50</sliderheight>
					<texturesliderbar>sliderback.png</texturesliderbar>
					<textureslidernib>slidernub.png</textureslidernib>
					<textureslidernibfocus>slidernubf.png</textureslidernibfocus>
				</control>
				<control type="label" id="14">
					<description>Default Label</description>
					<left>0</left>
					<top>0</top>
					<height>75</height>
					<label></label>
					<align>center</align>
					<aligny>center</aligny>
					<font>font25_title</font>
					<textcolor>grey</textcolor>
				</control>
				<control type="grouplist" id="9000">
					<left>1510</left>
					<top>92</top>
					<orientation>vertical</orientation>
					<width>300</width>
					<height>300</height>
					<itemgap>-10</itemgap>
					<onleft>5</onleft>
					<onright>3</onright>
				</control>
				<!-- OK 7 CANCEL BUTTONS -->
				<control type="grouplist">
				<top>950</top>
				<left>500</left>
				<width>1420</width>
				<align>left</align>
				<orientation>horizontal</orientation>
				<control type="button" id="28">
					<onup>5</onup>
					<ondown>5</ondown>
					<onleft>29</onleft>
					<onright>29</onright>
					<include>button_90</include>
				</control>
				<control type="button" id="29">
					<onup>5</onup>
					<ondown>5</ondown>
					<onleft>28</onleft>
					<onright>28</onright>
					<include>button_90</include>
				</control>
				</control>
			</control>
		</control>
	</controls>
</window>
