<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol always="true">450</defaultcontrol>
	<!-- 	1.32 FileBrowser.xml
ID 		Type 			Description
450 	container 		Directory list
451 	container 		List of available thumbnails
411 	label 			Heading label
412 	label 			Path of the selected item
413 	button 			OK button
414 	button 			Cancel button
415 	button 			Create folder
416 	radiobutton 	Flip Image horizontally  
-->
	<coordinates>
		<left>0</left>
		<top>0</top>
	</coordinates>
	<controls>
		<!-- default group -->
		<control type="group">
			<!--#######################################################################   MENU DA ESQUERDA   #######################################################################-->
			<control type="group">
				<animation effect="slide" start="1900,0" end="0,0" time="1000" tween="cubic" easing="out">WindowOpen</animation>
				<animation effect="slide" start="0,0" end="1900,0" time="1000" tween="cubic" easing="out">WindowClose</animation>
				<animation effect="slide" start="900,0" end="0,0" time="1000" tween="cubic" easing="out">Visible</animation>
				<animation effect="fade" start="100" end="0" time="400">Hidden</animation>
				<control type="image">
					<texture>dialog-bg.png</texture>
				</control>
				<control type="image">
					<right>100</right>
					<top>100</top>
					<width>540</width>
					<height>540</height>
					<fadetime>450</fadetime>
					<texture fallback="defaultfolder.png" background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio aligny="bottom">keep</aspectratio>
					<bordertexture border="-2">black.png</bordertexture>
				</control>
				<control type="image">
					<right>100</right>
					<top>650</top>
					<width>540</width>
					<height>540</height>
					<fadetime>450</fadetime>
					<texture flipy="true" diffuse="thumb_reflect.png" background="true">$INFO[ListItem.Icon]</texture>
					<aspectratio aligny="top">keep</aspectratio>
				</control>
				<control type="image">
					<left>100</left>
					<top>100</top>
					<width>1180</width>
					<height>90</height>
					<texture>colors/grey.png</texture>
					<animation effect="fade" end="20" condition="true">conditional</animation>
				</control>
				<control type="label" id="411">
					<!-- header label -->
					<left>150</left>
					<top>125</top>
					<width>1156</width>
					<height>90</height>
					<font>font30</font>
				</control>
				<control type="label">
					<!-- numero de itens -->
					<left>68</left>
					<top>975</top>
					<width>1156</width>
					<height>34</height>
					<font>font_plot</font>
					<label>$INFO[Container(450).CurrentPage]/$INFO[Container(450).NumPages]</label>
				</control>
				<!-- Common List -->
				<control type="list" id="450">
					<viewtype label="31801">list</viewtype>
					<left>100</left>
					<top>248</top>
					<width>1282</width>
					<height>705</height>
					<pagecontrol>80</pagecontrol>
					<onleft/>
					<onright>413</onright>
					<orientation>vertical</orientation>
					<scrolltime>450</scrolltime>
					<preloaditems>2</preloaditems>
					<itemlayout height="90">
						<control type="image">
							<left>50</left>
							<top>0</top>
							<width>1100</width>
							<height>1</height>
							<texture>line.png</texture>
						</control>
						<control type="label">
							<left>50</left>
							<top>0</top>
							<width>1100</width>
							<height>90</height>
							<font>font30</font>
							<aligny>center</aligny>
							<textcolor>white</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</itemlayout>
					<focusedlayout height="90">
						<control type="image">
							<left>0</left>
							<top>0</top>
							<width>1180</width>
							<height>90</height>
							<texture>colors/grey.png</texture>
							<animation>conditional</animation>
						</control>
						<control type="label">
							<left>50</left>
							<top>0</top>
							<width>1120</width>
							<height>90</height>
							<font>font30</font>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<include>labelcolorfocus</include>
						</control>
						<control type="label">
							<left>1134</left>
							<top>0</top>
							<width>128</width>
							<height>90</height>
							<font>font30</font>
							<align>center</align>
							<aligny>center</aligny>
							<include>labelcolorfocus</include>
							<label fallback="-">[UPPERCASE]$INFO[ListItem.Size][/UPPERCASE]</label>
						</control>
					</focusedlayout>
				</control>
				<control type="list" id="451">
					<viewtype label="31801">list</viewtype>
					<left>100</left>
					<top>248</top>
					<width>1282</width>
					<height>705</height>
					<pagecontrol>80</pagecontrol>
					<onleft/>
					<onright>413</onright>
					<orientation>vertical</orientation>
					<scrolltime>450</scrolltime>
					<preloaditems>2</preloaditems>
					<itemlayout height="90">
						<control type="image">
							<left>50</left>
							<top>0</top>
							<width>1100</width>
							<height>1</height>
							<texture>line.png</texture>
						</control>
						<control type="label">
							<left>50</left>
							<top>0</top>
							<width>1100</width>
							<height>90</height>
							<font>font30</font>
							<aligny>center</aligny>
							<textcolor>white</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</itemlayout>
					<focusedlayout height="90">
						<control type="image">
							<left>0</left>
							<top>0</top>
							<width>1180</width>
							<height>90</height>
							<texture>colors/grey.png</texture>
							<animation>conditional</animation>
						</control>
						<control type="label">
							<left>50</left>
							<top>0</top>
							<width>900</width>
							<height>90</height>
							<font>font30</font>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<include>labelcolorfocus</include>
						</control>
						<control type="label">
							<left>950</left>
							<top>0</top>
							<width>185</width>
							<height>90</height>
							<font>font30</font>
							<align>center</align>
							<aligny>center</aligny>
							<include>labelcolorfocus</include>
							<label fallback="-">[UPPERCASE]$INFO[ListItem.Size][/UPPERCASE]</label>
						</control>
					</focusedlayout>
				</control>
				<control type="grouplist" id="9001">
					<left>900</left>
					<top>900</top>
					<itemgap>0</itemgap>
					<onup>451</onup>
					<ondown>451</ondown>
					<orientation>vertical</orientation>
					<control type="button" id="413">
						<!-- OK button -->
						<left>0</left>
						<top>0</top>
						<width>488</width>
						<height>53</height>
						<align>center</align>
						<aligny>center</aligny>
						<font>font30</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<selectedcolor>FFF8F8F8</selectedcolor>
						<textoffsety>0</textoffsety>
						<textoffsetx>30</textoffsetx>
						<texture/>
						<texturefocus>colors/grey.png</texturefocus>
						<texturenofocus/>
						<pulseonselect>false</pulseonselect>
						<colordiffuse/>
						<label>$LOCALIZE[186]</label>
					</control>
					<control type="button" id="414">
						<!-- Cancel Button -->
						<left>0</left>
						<top>0</top>
						<width>488</width>
						<height>53</height>
						<align>center</align>
						<aligny>center</aligny>
						<font>font30</font>
						<textcolor>$INFO[skin.string(colortextlist)]</textcolor>
						<focusedcolor>E0101010</focusedcolor>
						<selectedcolor>FFF8F8F8</selectedcolor>
						<textoffsety>0</textoffsety>
						<textoffsetx>30</textoffsetx>
						<texture/>
						<texturefocus>colors/grey.png</texturefocus>
						<texturenofocus/>
						<pulseonselect>false</pulseonselect>
						<colordiffuse/>
						<label>$LOCALIZE[222]</label>
					</control>
					<control type="button" id="415">
						<!-- Create folder -->
						<left>0</left>
						<top>0</top>
						<width>488</width>
						<height>53</height>
						<align>center</align>
						<aligny>center</aligny>
						<font>font30</font>
						<textcolor>white</textcolor>
						<focusedcolor>black</focusedcolor>
						<selectedcolor>FFF8F8F8</selectedcolor>
						<textoffsety>0</textoffsety>
						<textoffsetx>30</textoffsetx>
						<texture/>
						<texturefocus>colors/grey.png</texturefocus>
						<texturenofocus/>
						<pulseonselect>false</pulseonselect>
						<colordiffuse/>
						<label>119</label>
					</control>
				</control>
				<control type="scrollbar" id="80">
					<include>ScrollBar_Vertical</include>
					<left>8</left>
					<top>102</top>
					<height>870</height>
					<onleft>9900</onleft>
					<onright>51</onright>
					<include>animation_fade</include>
					<visible allowhiddenfocus="true">Control.HasFocus(80)</visible>
				</control>
			</control>
		</control>
	</controls>
</window>
